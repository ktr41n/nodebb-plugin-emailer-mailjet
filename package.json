{
  "name": "nodebb-plugin-emailer-mailjet",
  "version": "2.0.0",
  "description": "An emailer plugin for NodeBB using Mailjet as a third party service",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktr41n/nodebb-plugin-emailer-mailjet"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "emailer",
    "mailjet"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "ktr41n",
    "email": "kenneth.tomplait@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ktr41n/nodebb-plugin-emailer-mailjet/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-angular": "^7.1.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^2.4.0",
    "lint-staged": "^8.2.0"
  },
  "dependencies": {
    "node-mailjet": "^5.0.0"
  }
}
